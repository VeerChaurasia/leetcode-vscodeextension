{
  "name": "leetcode-cph",
  "displayName": "leetcode-cph",
  "description": "Fetches LeetCode test cases and verifies input/output locally.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "leetcode-cph.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "leetcode-cph.fetchTestCases",
        "title": "Fetch Test Cases",
        "category": "Competitive Programming Helper"
      },
      {
        "command": "leetcode-cph.runTestCases",
        "title": "Run Test Cases",
        "category": "Competitive Programming Helper"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node-fetch": "^2.6.12",
    "jsdom": "^25.0.1",
    "node-fetch": "^3.3.2"
  }
}
